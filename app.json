{
  "expo": {
    "name": "Lab Management",
    "slug": "atomwalk-lab-management",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/adaptive-icons.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icons.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.atomwalk.atomwalklabmanagement"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/adaptive-icons.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/adaptive-icons.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "406e6076-e603-4cc4-8fc5-f2ce18356c5a"
      }
    },
    "owner": "atomwalk"
  }
}
